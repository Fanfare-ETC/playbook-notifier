{
  "$schema": "http://json-schema.org/draft-04/schema",
  "required": ["id", "plays"],
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1
    },
    "plays": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "number"
      }
    }
  }
}